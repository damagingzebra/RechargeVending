                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT Date_SK AS SaleDate, Time_SK AS SaleTime, 
       Machine_SK, Location_SK, Product_SK,
       Position, Slot, Tender.Name AS Tender, Price, Cost, LastItem
FROM Sales
INNER JOIN Stock ON Stock.StockID = Sales.StockID
INNER JOIN Tender ON Tender.TenderID = Sales.TenderID
INNER JOIN Product ON Product.ProductID = Stock.ProductID
INNER JOIN BuildingMachine ON BuildingMachine.MachineID = Stock.MachineID
-- Cross Join TO Get Dimension IDs
INNER JOIN RechargeDM.dbo.DimDate AS dd ON dd.Date = Sales.SaleDate
INNER JOIN RechargeDM.dbo.DimTime AS dt ON dt.Time = Sales.SaleTime
INNER JOIN RechargeDM.dbo.DimLocation AS dl ON dl.Location_SK = BuildingMachine.MachineID
INNER JOIN RechargeDM.dbo.DimMachine AS dm ON dm.Machine_SK = Stock.MachineID
INNER JOIN RechargeDM.dbo.DimProduct AS dp ON dp.Product_SK = Stock.ProductID</property>
